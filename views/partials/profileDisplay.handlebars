<div>
  <div class="row justify-content-end">
    <a href="/edit" class="text-decoration-none" id="edit-profile">
      <i class="cog fa-solid fa-gear fa-3x pr-4 pt-2"></i>
    </a>
  </div>
  <div class="justify-content-center text-center">
    <img
    {{#if userData.profile.image_url}}
      src={{userData.profile.image_url}}
      {{else}}
      src="https://i.imgur.com/HtPjfvn.png"
      {{/if}}
      class="card-img-top p-4"
      alt="profile-image"
    />
    <div class="card-body">
      <h2 class="card-title">{{userData.name}}</h2>
      <p class="card-text">Location: {{userData.profile.location}}</p>
      <p class="card-text">Height: {{userData.profile.height}} feet</p>
      <p class="card-text">Starting Weight:
        {{userData.profile.starting_weight}}
        lbs
      </p>
      <button class="btn btn-primary" id="deleteBtn">Delete Account</button>
    </div>
  </div>
</div>

<script src="/js/profiledisplay.js"></script>