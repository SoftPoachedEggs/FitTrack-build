<section class="container-fluid space-between p-4 row ps-2">
        <!-- Container for profile-pic -->
<div class="col-md-4 col-sm-12 mx-auto pr-1 border" id="user-profile-card">
<a href="https://www.example.com">
  <i class="fas fa-home"></i>
</a>
    <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/2/2c/Default_pfp.svg/1200px-Default_pfp.svg.png" class="card-img-top img-fluid p-5" alt="profile-image">
    <div class="card-body">
      <h2 class="card-title">{{userData.name}}</h2>
      <p class="card-text">Email: {{userData.email}} </p>
    </div>
  </div>
</div>
        <div class="col-md-8 col-sm-12 border p-5 mx-auto" id="chart-display">
            <div class="row justify-content-end">
                <button class="btn btn-outline-secondary" id="activityBtn">+ Log Activity</button>
            </div>
                    {{> activityGraph}}
        </div>
    </div>
</section>

<script src="/js/profile.js"></script>

    
