 <!-- If logged in main body of site -->
    <section class="container-fluid mt-4">
        <div class="row">
            <!-- Container for profile-pic -->
            <div class="col-lg-4">
                <div class="card" style="width: 18rem;">
                    <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/2/2c/Default_pfp.svg/1200px-Default_pfp.svg.png" class="card-img-top" alt="profile-image">
                    <div class="card-body">
                    <h2 class ="card-title">{{userData.username}}</h2>
                    <p class="card-text">Email: {{userData.email}} </p>
                    
                    </div>
                </div>
            </div>

            <div class="col-lg-8">
                {{> activityGraph}}
            </div>
            <div>
                <button class="btn btn-primary" id="logoutBtn">Logout</button>
            </div>
        </div>
    </section>

<script src="/js/profile.js"></script>

    
